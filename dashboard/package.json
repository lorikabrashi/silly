{
  "name": "silly_admin_dashboard",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --port 8080",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "start": "vue-cli-service serve --port 8080"
  },
  "dependencies": {
    "animate.css": "^3.6.1",
    "awesome-bootstrap-checkbox": "^1.0.1",
    "axios": "^0.19.2",
    "bootstrap": "^4.5.2",
    "bootstrap-vue": "2.0.3",
    "css-loader": "^1.0.0",
    "expose-loader": "^0.7.5",
    "font-awesome": "4.7.0",
    "glyphicons-halflings": "^1.9.1",
    "imports-loader": "^0.8.0",
    "line-awesome": "1.3.0",
    "node-sass": "^7.0.0",
    "regenerator-runtime": "^0.13.7",
    "sass-loader": "^7.0.1",
    "vue": "^2.6.12",
    "vue-cookies": "^1.7.4",
    "vue-router": "^3.4.3",
    "vue-tables-2": "^2.0.34",
    "vue-toasted": "^1.1.27",
    "vue-touch": "2.0.0-beta.4",
    "vuex": "^3.5.1",
    "webpack-cli": "^3.3.12"
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.11.0",
    "@vue/cli-plugin-babel": "^3.12.0",
    "@vue/cli-plugin-eslint": "^3.12.0",
    "@vue/cli-plugin-unit-jest": "~4.4.0",
    "@vue/cli-service": "^3.12.0",
    "@vue/test-utils": "^1.0.5",
    "vue-template-compiler": "^2.6.12"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.{test,spec}.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
