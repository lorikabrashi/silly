{
    "env": {
        "commonjs": true,
        "es2020": true,
        "node": true
    },
    "extends": [
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": 11
    },
    "rules": {
		"semi": "off",
        "comma-dangle": "off",
        "camelcase": "off",
        "eqeqeq": "off",
        "indent": ["error", 4,  { "SwitchCase": 1 }]
    }
}
